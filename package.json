{
  "name": "yawc",
  "version": "0.0.1",
  "description": "Minimal webpack config maker",
  "main": "lib/webpack-config.js",
  "scripts": {
    "build": "babel src --out-dir lib --copy-files --loose",
    "cover": "istanbul cover _mocha -- --compilers js:babel/register -R spec",
    "lint": "eslint src/",
    "prepublish": "node ./utils/prepublish.js",
    "test": "mocha --compilers js:babel/register",
    "watch": "babel src --out-dir lib --copy-files --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/boopathi/yawc"
  },
  "engines": {
    "node": ">=0.10.0",
    "npm": ">=3.0.0"
  },
  "keywords": [
    "config maker",
    "webpack"
  ],
  "license": "Apache-2.0",
  "bugs": "https://github.com/boopathi/yawc/issues",
  "homepage": "https://github.com/boopathi/yawc",
  "dependencies": {
    "babel-core": "~6.23.1",
    "babel-loader": "~6.3.2",
    "css-loader": "~0.26.1",
    "cssnext": "~1.8.4",
    "express": "~4.14.1",
    "express-http-proxy": "~0.11.0",
    "extract-text-webpack-plugin": "~1.0.1",
    "image-size-loader": "~0.7.0",
    "imports-loader": "~0.7.0",
    "json-loader": "~0.5.2",
    "node-libs-browser": "~2.0.0",
    "postcss-loader": "^1.3.1",
    "postcss-mixins": "^5.4.1",
    "postcss-nested": "^1.0.0",
    "postcss-simple-vars": "^3.0.0",
    "promise": "~7.1.1",
    "react-hot-loader": "~1.3.0",
    "style-loader": "~0.13.1",
    "webpack": "~2.2.1",
    "webpack-dev-middleware": "~1.10.1",
    "webpack-dev-server": "~2.4.1"
  },
  "devDependencies": {
    "babel": "~6.23.0",
    "babel-eslint": "~7.1.1",
    "eslint": "~3.16.0",
    "eslint-config-phrontend": "0.0.3",
    "istanbul": "~0.4.5",
    "mocha": "~3.2.0",
    "should": "~11.2.0"
  }
}
